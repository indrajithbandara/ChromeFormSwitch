{
  "content_scripts": [ {
    //      "matches": [ "https://docs.google.com/*" ],
    "matches": [ "<all_urls>" ],
    "all_frames": true,
    "css": [
      "css/jquery-ui.css",
      "css/keyboard.css",
      "src/inject.css"
    ],
    "js": [
      "js/jquery.js",
      "js/jquery-ui.min.js",
      "js/jquery.keyboard.js",
      "js/jquery.keyboard.extension-switch-navigation.js",
      "src/inject.js"
    ],
    "run_at": "document_end"
  } ],
  "description": "Switch accessibility for Google Forms",
  "manifest_version": 2,
  "name": "Switch Access for Google Forms",
  "permissions": [ "tabs", "<all_urls>" ],
  "version": "1.0.0",
  "web_accessible_resources": [
    "images/*.png",
    "css/jquery-ui.css",
    "js/jquery.js",
    "js/jquery-ui.min.js",
    "css/keyboard.css",
    "js/jquery.keyboard.js",
    "js/jquery.keyboard.extension-switch-navigation.js",
    "src/inject.css",
    "src/inject.js"
  ]
}
